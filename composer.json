{
    "name": "mburakdndr/fcm-notifications",
    "description": "Flarum i√ßin Firebase Cloud Messaging bildirimleri",
    "type": "flarum-extension",
    "require": {
        "flarum/core": "^1.8.0",
        "kreait/firebase-php": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "mburakdndr\\FcmNotifications\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "FCM Notifications",
            "category": "feature",
            "icon": {
                "name": "fas fa-bell",
                "backgroundColor": "#238c59",
                "color": "#fff"
            }
        },
        "flarum-cli": {
            "modules": {
                "src/Console/Commands/*Command.php": "command"
            }
        }
    }
}